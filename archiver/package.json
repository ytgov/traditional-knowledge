{
  "name": "traditional-knowledge-archiver",
  "private": true,
  "version": "0.0.1",
  "description": "TK Archiver",
  "main": "src/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rm -rf ./dist",
    "start": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "start:scheduler": "ts-node-dev -r tsconfig-paths/register src/scheduler.ts",
    "ts-node": "ts-node -r tsconfig-paths/register --logError",
    "test": "vitest",
    "lint": "eslint . --ext .js,.ts --ignore-path ../.gitignore",
    "check-types": "tsc -p tests/tsconfig.json --noEmit"
  },
  "author": "Marlen Brunner",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.26.0",
    "@sequelize/core": "^7.0.0-alpha.40",
    "@sequelize/mssql": "^7.0.0-alpha.40",
    "axios": "^1.13.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "knex": "^3.1.0",
    "libreoffice-convert": "^1.6.0",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "morgan": "^1.10.0",
    "mssql": "^11.0.1",
    "node-schedule": "^2.1.1",
    "redis": "^4.7.0",
    "uuid": "^11.0.2",
    "winston": "^3.14.1",
    "winston-cloudwatch": "^6.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.1",
    "@types/luxon": "^3.4.2",
    "@types/morgan": "^1.9.9",
    "@types/node-schedule": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^4.0.18"
  }
}
